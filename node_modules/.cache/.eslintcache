         </div>\n\n            {/* RATING */}\n            <div className=\"row my-5\">\n              <div className=\"col-md-6\">\n                <h6 className=\"mb-3\">REVIEWS</h6>\n                {product.reviewQuantity === 0 && (\n                  <Message variant={\"alert-info mt-3\"}>No Reviews</Message>\n                )}\n                {reviews &&\n                  reviews?.map((review) => (\n                    <div className=\"mb-5 mb-md-3 bg-light p-3 shadow-sm rounded\">\n                      <strong>{review.name}</strong>\n                      <Rating value={review.rating} />\n                      <span>{moment(review.date).calendar()}</span>\n                      <div className=\"alert alert-info mt-3\">\n                        {review.comment}\n                      </div>\n                    </div>\n                  ))}\n              </div>\n              <Pagination\n                currentPage={page}\n                pageTotal={pageTotal}\n                productId={productId}\n              />\n            </div>\n          </>\n        )}\n      </div>\n    </>\n  );\n};\n\nexport default SingleProduct;\n","E:\\website\\React\\Clone3\\Shoe\\FETT2\\EcomWeb22\\src\\screens\\ProfileScreen.js",["234"],"import moment from \"moment/moment\";\nimport React, { useEffect } from \"react\";\nimport { useDispatch, useSelector } from \"react-redux\";\n\nimport Header from \"../components/Header\";\nimport ProfileTabs from \"../components/profileComponents/ProfileTabs\";\nimport { listOrders } from \"../Redux/Action/OrderAction\";\nimport { getUserDetails, login } from \"../Redux/Action/UserAction\";\nimport Orders from \"./../components/profileComponents/Orders\";\n\nconst ProfileScreen = () => {\n  window.scrollTo(0, 0);\n\n  const userDetails = useSelector((state) => state.userDetails);\n  const { userInfo } = userDetails;\n  const orderList = useSelector((state) => state.orderList);\n  const { loading, error, orders } = orderList;\n  const 